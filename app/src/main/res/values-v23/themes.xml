<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.ProjekPemmob" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/red_main</item>
        <item name="colorPrimaryVariant">@color/red_main</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/orange_main</item>
        <item name="colorSecondaryVariant">@color/orange_main</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/background_color_light</item>
        <!-- Customize your theme here. -->
        <item name="android:windowBackground">@color/background_color_light</item>
        <item name="android:windowLightStatusBar">true</item>
    </style>
</resources>